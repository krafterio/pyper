<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_product_network_tree" model="ir.ui.view">
        <field name="name">product.network.tree</field>
        <field name="model">product.network</field>
        <field name="arch" type="xml">
            <tree multi_edit="1" editable="top">
                <field name="sequence" widget="handle"/>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_product_network" model="ir.actions.act_window">
        <field name="name">Product network</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">product.network</field>
        <field name="view_mode">tree</field>
    </record>

</odoo>
