<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_product_template_features_form" model="ir.ui.view">
        <field name="name">product.template.features.form.inherit</field>
        <field name="model">product.template</field>
        <field name="priority">0</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[@name='variants']" position="before">
                <page string="Product features" name="page_product_features"/>
            </xpath>
        </field>
    </record>
</odoo>