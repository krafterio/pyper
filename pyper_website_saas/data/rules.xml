<?xml version="1.0" encoding="utf-8"?>
<data noupdate="0">
    <record id="base.open_module_tree" model="ir.actions.act_window">
        <field name="domain">[('to_buy', '=', False), '!', ('name', '=like', 'theme_%')]</field>
    </record>

    <record model="ir.rule" id="payment_provider_saas_rule">
        <field name="name">SaaS Available Payment Provider</field>
        <field name="model_id" search="[('model','=','payment.provider')]" model="ir.model"/>
        <field name="domain_force">[('module_to_buy', '=', False)]</field>
    </record>
</data>
