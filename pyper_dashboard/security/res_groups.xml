<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="0">
    <record id="module_dashboard_root" model="ir.module.category">
        <field name="name">Dashboard</field>
        <field name="sequence">100</field>
    </record>

    <record id="module_dashboard" model="ir.module.category">
        <field name="name">Dashboard</field>
        <field name="sequence">100</field>
        <field name="parent_id" ref="module_dashboard_root"/>
    </record>

    <record id="group_dashboard" model="ir.module.category">
        <field name="name">Dashboard</field>
        <field name="sequence">100</field>
        <field name="parent_id" ref="module_dashboard"/>
    </record>

    <record id="group_dashboard_user" model="res.groups">
        <field name="category_id" ref="module_dashboard"/>
        <field name="name">User</field>
    </record>

    <record id="group_dashboard_admin" model="res.groups">
        <field name="category_id" ref="module_dashboard"/>
        <field name="name">Administrator</field>
        <field name="implied_ids" eval="[(4, ref('group_dashboard_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
</odoo>
