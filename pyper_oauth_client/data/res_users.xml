<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <record id="template_internal_user_id" model="res.users">
        <field name="name">Internal User Template</field>
        <field name="login">internaluser</field>
        <field name="active" eval="False"/>
        <field name="notification_type">inbox</field>
        <field name="groups_id" eval="[Command.set([ref('base.group_user')])]"/>
    </record>

    <record id="default_template_user_config" model="ir.config_parameter">
        <field name="key">oauth_client.template_internal_user_id</field>
        <field name="value" ref="template_internal_user_id"/>
    </record>
</odoo>