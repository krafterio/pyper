<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="pyper_drawer_workspace.Drawer.Header" t-inherit="pyper_drawer.Drawer.Header" t-inherit-mode="extension">
        <xpath expr="//div[hasclass('o_drawer_app_logo')]" position="attributes">
            <attribute name="t-if">false</attribute>
        </xpath>

        <xpath expr="." position="inside">
            <div class="o_main_navbar o_drawer_workspace_menu" role="menu">
                <WorkspaceMenu/>
            </div>
        </xpath>
    </t>

</templates>
