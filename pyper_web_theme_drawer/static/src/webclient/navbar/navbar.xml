<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-name="pyper_web_theme_drawer.NavBar" t-inherit="web.NavBar" t-inherit-mode="extension">
        <xpath expr="//Drawer" position="inside">
            <t t-set-slot="header">
                <div class="o_main_navbar">
                    <DrawerAppMenu>
                        <t t-set-slot="minified">
                            <img src="/pyper_web_theme/static/img/icon.svg" width="50" height="50" class="rounded-3"/>
                        </t>

                        <t t-set-slot="locked">
                            <img src="/pyper_web_theme/static/img/icon.svg" width="50" height="50" class="rounded-3"/>
                        </t>
                    </DrawerAppMenu>
                </div>
            </t>
        </xpath>
    </t>
</templates>
