<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="pyper_drawer.NavBar" t-inherit="web.NavBar" t-inherit-mode="extension">
        <xpath expr="//header[hasclass('o_navbar')]" position="before">
            <Drawer/>
        </xpath>

        <xpath expr="//t[@t-call='web.NavBar.AppsMenu']" position="after">
            <DrawerToggler/>
        </xpath>
    </t>

</templates>
