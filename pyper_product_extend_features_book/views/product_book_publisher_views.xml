<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_product_book_publisher_tree" model="ir.ui.view">
        <field name="name">doc.page.tree</field>
        <field name="model">product.book.publisher</field>
        <field name="arch" type="xml">
            <tree multi_edit="1">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_product_book_publisher_page" model="ir.actions.act_window">
        <field name="name">Publishers</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">product.book.publisher</field>
        <field name="view_mode">tree</field>
    </record>

</odoo>
