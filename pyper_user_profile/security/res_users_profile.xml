<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <record id="admin_profile" model="res.users.profile">
        <field name="name">Administrator</field>
        <field name="role_ids" eval="[
            Command.link(ref('pyper_user_profile.manage_user_profiles_role')),
            Command.link(ref('pyper_user_profile.internal_user_role')),
