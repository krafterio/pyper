#. module: pyper_mail_ai
#: model:mail.template.ai.config,instruction_content:pyper_mail_ai.mail_template_ai_config_email
msgid ""
"<?xml version=\"1.0\"?>\n"
"<data><h1>Instruction</h1>\n"
"            <p>\n"
"                The answer:\n"
"                <br/>\n"
"                <ul>\n"
"                    <li>must be in HTML format containing only HTML tags dedicated to text formatting</li>\n"
"                    <li>should only contain the content of the message</li>\n"
"                    <li>must not refer to the prompt sent</li>\n"
"                    <li>must not start with \"html\" or \"'\"</li>\n"
"                    <li>must not add sender information at the end of the message</li>\n"
"                    <li>should use the person's first name if possible</li>\n"
"                    <li>must remove emojis present in the contact function</li>\n"
"                    <li>must remove references to the company in the contact function (starts with \"at...\")</li>\n"
"                    <li>must format the company name in a classic syntax with a first capital letter and the rest in lowercase</li>\n"
"                    <li>must not contain replacement keywords</li>\n"
"                    <li>must remove the signature or references to my personal information at the end of the message</li>\n"
"                    <li>must not include a placeholder dedicated to the message signature</li>\n"
"                </ul>\n"
"            </p>\n"
"        </data>"
msgstr ""
"            <h1>Instruction</h1>\n"
"            <p>\n"
"                La réponse :\n"
"                <br/>\n"
"                <ul>\n"
"                    <li>doit être au format HTML contenant que des tags HTML dédié au formattage du texte</li>\n"
"                    <li>ne doit contenir que le contenu du message</li>\n"
"                    <li>ne doit pas faire allusion au prompt envoyé</li>\n"
"                    <li>ne doit pas commencer par \"html\" ou \"'\"</li>\n"
"                    <li>ne doit pas ajouter des informations de l’expéditeur en fin de message</li>\n"
"                    <li>doit utiliser le prénom de la personne si possible</li>\n"
"                    <li>doit retirer les emojis présent dans la fonction du contact</li>\n"
"                    <li>doit retirer les références vers la société dans la fonction du contact (commence par \"chez…\")</li>\n"
"                    <li>doit formater le nom de la société dans une syntaxe classique avec une première majuscule et le reste en minuscule</li>\n"
"                    <li>ne doit pas contenir de mots clés de remplacement</li>\n"
"                    <li>doit supprimer la signature ou les références à mes informations personnelles à la fin du message</li>\n"
"                    <li>ne doit pas inclure de placeholder dédié à la signature du message</li>\n"
"                </ul>\n"
"            </p>"

#. module: pyper_mail_ai
#: model:mail.template.ai.model,model_information:pyper_mail_ai.mail_template_ai_model_partner
msgid ""
"<?xml version=\"1.0\"?>\n"
"<data><p>\n"
"                Contact information to use:\n"
"            </p>\n"
"\n"
"            <ul>\n"
"                <li>Name: <t t-out=\"object.name\"/></li>\n"
"                <li>Title: <t t-out=\"object.function\"/></li>\n"
"                <li>Email: <t t-out=\"object.email\"/></li>\n"
"                <li>Mobile phone: <t t-out=\"object.mobile\"/></li>\n"
"                <li>Phone: <t t-out=\"object.phone\"/></li>\n"
"                <li>Company: <t t-out=\"object.parent_id.name\"/></li>\n"
"                <li>Company description: <t t-out=\"object.parent_id.comment\"/></li>\n"
"            </ul>\n"
"        </data>"
msgstr ""
"            <p>\n"
"                Informations du contact à utiliser :\n"
"            </p>\n"
"\n"
"            <ul>\n"
"                <li>Nom : <t t-out=\"object.name\"/></li>\n"
"                <li>Titre : <t t-out=\"object.function\"/></li>\n"
"                <li>Email: <t t-out=\"object.email\"/></li>\n"
"                <li>Téléphone mobile : <t t-out=\"object.mobile\"/></li>\n"
"                <li>Téléphone : <t t-out=\"object.phone\"/></li>\n"
"                <li>Société : <t t-out=\"object.parent_id.name\"/></li>\n"
"                <li>Description de la société : <t t-out=\"object.parent_id.comment\"/></li>\n"
"            </ul>"

#. module: pyper_mail_ai
#: model:mail.template.ai.config,instruction_subject:pyper_mail_ai.mail_template_ai_config_email
msgid ""
"<?xml version=\"1.0\"?>\n"
"<p>\n"
"                The answer:\n"
"                <br/>\n"
"                <ul>\n"
"                    <li>must always be in plain text format</li>\n"
"                    <li>should never start with a prefix like \"Object: \"</li>\n"
"                    <li>should not include reference to contact information such as company or position</li>\n"
"                    <li>must be a short subject of the message of less than 20 words in French format without a period at the end intended for contact</li>\n"
"                </ul>\n"
"            </p>\n"
"        "
msgstr ""
"            <p>\n"
"                La réponse :\n"
"                <br/>\n"
"                <ul>\n"
"                    <li>doit toujours être au format texte brut</li>\n"
"                    <li>ne doit jamais commencer par un préfix comme par exemple \"Object: \"</li>\n"
"                    <li>ne doit pas inclure de référence aux informations du contact tel que sa société ou son poste</li>\n"
"                    <li>doit être un sujet court du message de moins de 20 mots au format français sans point à la fin destiné au contact</li>\n"
"                </ul>\n"
"            </p>"

